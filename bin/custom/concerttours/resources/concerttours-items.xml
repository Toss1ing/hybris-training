<?xml version="1.0" encoding="ISO-8859-1"?>
<items xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="items.xsd">

    <collectiontypes>
        <collectiontype code="CustomStringSet" elementtype="java.lang.String" type="set"/>
    </collectiontypes>

    <enumtypes>
        <enumtype code="ConcertType" dynamic="false">
            <value code="OPENAIR"/>
            <value code="INDOOR"/>
        </enumtype>

        <enumtype code="MusicType" dynamic="true">
        </enumtype>
    </enumtypes>

    <maptypes>
        <maptype code="SocialMediaLinks" argumenttype="java.lang.String" returntype="java.lang.String"/>
    </maptypes>

    <relations>
        <relation code="Product2RockBand" localized="false">
            <sourceElement qualifier="tours" type="Product" collectiontype="set" cardinality="many" ordered="false">
                <modifiers read="true" write="true" search="true" optional="true"/>
            </sourceElement>
            <targetElement qualifier="band" type="Band" cardinality="one">
                <modifiers read="true" write="true" search="true" optional="true"/>
            </targetElement>
        </relation>
        <relation code="Band2MusicType" localized="false">
            <deployment table="Band2MusicType" typecode="30269"/>
                <sourceElement qualifier="bands" type="Band" collectiontype="set" cardinality="many" ordered="false">
                <modifiers read="true" write="true" search="true" optional="true"/>
            </sourceElement>
            <targetElement qualifier="types" type="MusicType" cardinality="many">
                <modifiers read="true" write="true" search="true" optional="true"/>
            </targetElement>
        </relation>
    </relations>

    <itemtypes>
        <itemtype code="Product" autocreate="false">
            <attributes>
                <attribute qualifier="hashtag" type="java.lang.String">
                    <description>hashtag of concert tour for social media</description>
                    <persistence type="property"/>
                </attribute>
            </attributes>
        </itemtype>

        <itemtype code="Band">
            <deployment table="Band" typecode="10102"/>
            <attributes>
                <attribute qualifier="code" type="java.lang.String">
                    <description>short unique code of band</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="name" type="java.lang.String">
                    <description>name of band</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="history" type="java.lang.String">
                    <description>history of band</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="albumSales" type="java.lang.Long">
                    <description>official global album sales</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="bandDetails" type="BandDetails">
                    <description>band details</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="socialMediaLinks" type="SocialMediaLinks">
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="albumSet" type="CustomStringSet">
                    <persistence type="property"/>
                </attribute>
            </attributes>
        </itemtype>

        <itemtype code="Concert" extends="VariantProduct">
            <attributes>
                <attribute qualifier="venue" type="java.lang.String">
                    <description>venue of concert</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="date" type="java.util.Date">
                    <description>date of concert</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="concertType" type="ConcertType">
                    <description>type of concert (indoors or open air)</description>
                    <persistence type="property"/>
                </attribute>
            </attributes>
        </itemtype>

        <itemtype code="BandDetails">
            <deployment table="BandDetails" typecode="10103"/>
            <attributes>
                <attribute qualifier="code" type="java.lang.String">
                    <description>short unique code of band</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="listeners" type="java.lang.Double">
                    <description>count listeners</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="topCountryFroSales" type="java.lang.String">
                    <description>country where band sell more tickets</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="rating" type="java.lang.Short">
                    <description>rating of band</description>
                    <persistence type="property"/>
                </attribute>
            </attributes>
        </itemtype>
    </itemtypes>
</items>
